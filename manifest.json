{
    "name": "Hack Cambridge 2022",
    "description": "It should work lmao",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["activeTab", "geolocation", "notifications", "<all_urls>", "tabs"],

    "background": {
        "scripts": ["bg.js"]
    },

    "web_accessible_resources" : [
        "bg.js"
